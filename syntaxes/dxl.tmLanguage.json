				{
					"name": "constant.language.dxl",
					"match": "\\b(false|off|on|true)\\b"
				},
				{
					"name": "constant.numeric.dxl",
					"match": "\\b([+-]?[0-9.]+|[0-9.]+(e|E)[0-9]+)\\b"
				}
			]
