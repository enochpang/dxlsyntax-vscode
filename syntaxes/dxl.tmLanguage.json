{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "DXL",
	"patterns": [
		{"include": "#comments"},
		{"include": "#strings"},
		{"include": "#dxlInvalid"},
		{"include": "#dxlConstants"},
		{"include": "#dxlKeywords"},
		{"include": "#dxlTypes"},
		{"include": "#dxlSupportFunctions"},
		{"include": "#dxlSupportConstants"},
		{"include": "#dxlSupportVariables"}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"name": "comment.dxl",
					"begin": "//",
					"end": "$"
				},
				{
					"name": "comment.dxl",
					"begin": "/\\*",
					"end": "\\*/"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.dxl",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.dxl",
					"match": "\\\\."
				}
			]
		},
		"dxlInvalid": {
			"patterns": [
				{
					"name": "invalid.illegal.dxl",
					"match": "\\b(case|const|default|enum|struct|switch|union)\\b"
				}
			]
		},
		"dxlKeywords": {
			"patterns": [
				{
					"name": "keyword.dxl",
					"match": "\\b(and|module|object|or|pragma|sizeof|static|void)\\b"
				},
				{
					"name": "keyword.control.dxl",
					"match": "\\b(by|do|else|if|for|in|then|while|break|continue|halt|return)\\b"
				},
				{
					"name": "keyword.declaration.dxl",
					"match": "^(#include|pragma)"
				}
			]
		},
		"dxlTypes": {
			"patterns": [
				{
					"name": "storage.type.dxl",
					"match": "\\b(bool|char|int|null|real|string)\\b"
				},
				{
					"name": "storage.type.dxl",
					"match": "\\b(AccessRec|AlternativeWord|ArchiveData|ArchiveItem|AttrBaseType|AttrDef|AttrType|BE|Baseline|BaselineSet|BaselineSetDefinition|Buffer|Column|Comment|ConfStream|DB|DBE|Dictionary|Discussion|DiscussionStatus|DropEvent|EmbeddedOleObject|ExternalLink|ExternalLinkBehavior|Filter|Folder|Future|GrammarRules|Group|GroupList|History|HistorySession|HistoryType|HttpBody|HttpDelete|HttpGet|HttpHead|HttpHeader|HttpHeaderEntry|HttpPost|HttpPut|HttpRequest|HttpResponse|HttpVerb|IPC|Icon|InPartition|IntegrityCheckItem|IntegrityItemType|IntegrityProblem|IntegrityResultsData|Item|Justification|Languages|LdapItem|Link|LinkFilter|LinkModuleDescriptor|LinkRef|Linkset|Locale|Lock|LockList|LoginPolicy|ModName_|Module|ModuleProperties|ModuleVersion|OSLCDXLService|Object|OleAutoArgs|OleAutoObj|OutPartition|PageLayout|PartitionAttribute|PartitionDefinition|PartitionFile|PartitionModule|PartitionPermission|PartitionView|Permission|ProblemItem|Project|Regexp|RichText|RichTextParagraph|RifDefinition|RifImport|RifModuleDefinition|ScrollEvent|ScrollSide|Sensitivity|SignatureEntry|SignatureInfo|SignatureInfoSpecifier__|Skip|Sort|SpellingAlternatives|SpellingError|SpellingOptions|Stat|Stream|TableBorderPosition|TableBorderStyle|Template|ToolCombo|ToolEditableCombo|ToolType|Trigger|TriggerStatus|User|UserList|UserNotifyList|View|ViewDef)\\b"
				}
